{
    "definitions": {
        "ClairdashNodeConfig": {
            "allOf": [
                {
                    "type": "object",
                    "properties": {
                        "meta": {
                            "$ref": "https://schemas.clairdash.com/clairdash/metadata.json#/definitions/ClairdashModelMetadata"
                        }
                    }
                }
            ]
        },
        "ClairdashColumnInfo": {
            "allOf": [
                {
                    "type": "object",
                    "properties": {
                        "meta": {
                            "$ref": "https://schemas.clairdash.com/clairdash/metadata.json#/definitions/ClairdashColumnMetadata"
                        }
                    }
                }
            ]
        },
        "ClairdashCompiledModelNode": {
            "allOf": [
                {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string",
                            "allOf": [
                                {
                                    "not": { "enum": ["clairdash", "ld", ""] }
                                },
                                {
                                    "pattern": "^[^.]*$"
                                }
                            ]
                        },

                        "meta": {
                            "$ref": "https://schemas.clairdash.com/clairdash/metadata.json#/definitions/ClairdashModelMetadata"
                        },
                        "config": {
                            "$ref": "#/definitions/ClairdashNodeConfig"
                        },
                        "columns": {
                            "type": "object",
                            "additionalProperties": {
                                "$ref": "#/definitions/ClairdashColumnInfo"
                            },
                            "default": {}
                        }
                    }
                }
            ]
        }
    },
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://schemas.clairdash.com/clairdash/v12.json"
}
